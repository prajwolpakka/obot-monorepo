# Application
PORT=4001
NODE_ENV=development
BACKEND_URL=http://localhost:4001
FRONTEND_URL=http://localhost:4000

# In-process AI (Embeddings + RAG)
# Voyage embeddings
VOYAGE_API_KEY=
VOYAGE_BASE_URL=https://api.voyageai.com/v1
EMBEDDINGS_MODEL=voyage-3.5-lite
